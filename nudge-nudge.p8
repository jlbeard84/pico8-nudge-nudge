pico-8 cartridge // http://www.pico-8.com
version 15
__lua__
--nudge-nudge
-- by jbeard

--gamestates:1=load,2=transitionin,3=game,4=transitionout

level=1
gridsquare=8
floorsprite=7
restartcount=0
isrestart=false
blockrows=15
blockcols=16
gamestate=1
stateincrementor=0
stateloadmax=90
statetransitionmax=127

player={}
player.x=0
player.y=0
player.initx=0
player.inity=0
playerspr=1
playersprdown=1
playersprup=8
playersprleft=10
playersprright=9

blockcount=0
blocks={}
initialblocks={}

portal={}
portal.x=0
portal.y=0
portalsprites={2,3,4}
portalspritelength=3
portalcounter=1
portalspriteiterator=1
portalspritespeed=5

rowstart=0
colstart=0
rowoffset=0
coloffset=0

function calcxoffset(iterator)
	return (iterator-((level-1)*blockcols))*gridsquare
end

function calcyoffset(iterator)
	return (iterator)*gridsquare
end

function _init()

	blocks={}
	blockcount=0
	
	if isrestart==false then
		initialblocks={}
		portal={}
		player={}
		
		-- todo: adjust after i know how many
		-- levels fit per row in map
		rowoffset=15
		coloffset=((level-1)*blockcols)+blockcols
		
		rowstart=rowoffset-blockrows
		colstart=coloffset-blockcols

		for i=colstart,coloffset,1 do
 		for j=rowstart,rowoffset,1 do
 		
 		sprite=mget(i,j)
 		 
 			if fget(sprite,1)==true then
 				mset(i,j,7)
 				blockcount+=1
 				add(blocks,{x=calcxoffset(i),y=calcyoffset(j) })
				add(initialblocks,{x=calcxoffset(i),y=calcyoffset(j) })
 			elseif fget(sprite,2)==true then
 				mset(i,j,7)
 				portal.x=calcxoffset(i)
 				portal.y=calcyoffset(j)
 			elseif fget(sprite,3)==true then
 				mset(i,j,7)
 				player.x=calcxoffset(i)
 				player.y=calcyoffset(j)
 				player.initx=calcxoffset(i)
 				player.inity=calcyoffset(j)
 			end
 		end
 	end
 else
 
 	player.x=player.initx
 	player.y=player.inity
 	
 	for initblock in all(initialblocks) do
 		blockcount+=1
 		add(blocks,{x=initblock.x,y=initblock.y})	
 	end
 
 end

end

function getsprrelxpos(x)
	return (x/gridsquare)+((level-1)*blockcols)
end

function getsprrelypos(y)
	return y/gridsquare
end

function moveplayer()
 
 mvmtx=0
 mvmty=0
 
 if btnp(0) then
	mvmtx-=gridsquare
	playerspr=playersprleft
 elseif btnp(1) then
	mvmtx+=gridsquare
	playerspr=playersprright
 elseif btnp(2) then
  	mvmty-=gridsquare
  	playerspr=playersprup
 elseif btnp(3) then
  	mvmty+=gridsquare
  	playerspr=playersprdown
 end
 
 if mvmtx==0 and mvmty==0 then
 	return
 end

 player.x+=mvmtx
 player.y+=mvmty

 solidblock=fget(mget(getsprrelxpos(player.x),getsprrelypos(player.y)),0)
 
 for block in all(blocks) do
 
		if checkboundingcollision(player,block)==true then
					
				block.x+=mvmtx
				block.y+=mvmty
				
				if checkboundingcollision(block,portal) then
					del(blocks,block)
					blockcount-=1
					
					if blockcount==0 then
						levelfinished=true
					end
					
					return
				end

				blockpossolid=fget(mget(getsprrelxpos(block.x),getsprrelypos(block.y)),0)

				if blockpossolid==true then
					block.x-=mvmtx
					block.y-=mvmty
					player.x-=mvmtx
					player.y-=mvmty
							
					return		
				end
				
				for adjacantblock in all(blocks) do
					if block~=adjacantblock and
					 checkboundingcollision(block,adjacantblock)==true then
						
							block.x-=mvmtx
							block.y-=mvmty
							player.x-=mvmtx
							player.y-=mvmty
							
							return				
					end
				end
				
			
				if checkedgecollision(block)==true or solidblock==true then
					player.x-=mvmtx
					player.y-=mvmty
				end
			
				return
		end
 end

 checkedgecollision(player)

 if solidblock==true then
 	player.x-=mvmtx
 	player.y-=mvmty
 end
end

function checkboundingcollision(left,right)

	if left.x < right.x+gridsquare and
 	left.x+gridsquare > right.x and
 	left.y < right.y+gridsquare and
 	left.y+gridsquare > right.y then
 
 		return true
 end
 
 return false
end

function checkedgecollision(actor)
	if actor.x < 0 then
 	actor.x=0
 	sfx(0)
 	return true
 elseif actor.x > 127 then
 	actor.x=120
 	sfx(0)
 	return true
 elseif actor.y < 0 then
 	actor.y=0
 	sfx(0)
 	return true
 elseif actor.y > 119 then
 	actor.y=112
 	sfx(0)
 	return true
 end
 
 return false
end

function updateanimations()
	portalspriteiterator+=1
	
	if portalspriteiterator>portalspritespeed then
		portalspriteiterator=1
		portalcounter+=1
		
		if portalcounter>portalspritelength then
			portalcounter=1
		end
	end
end

function _update()

	if gamestate==1 then
		stateincrementor+=1

		if stateincrementor > stateloadmax then
			stateincrementor=0
			gamestate=2
		end

	elseif gamestate==2 then
		stateincrementor+=12

		if stateincrementor > statetransitionmax then
			stateincrementor=0
			gamestate=3
		end

	elseif gamestate==3 then

		if blockcount==0 then
			isrestart=false
			gamestate=4
			return
		end

		if btnp(5) then
			isrestart=true
			restartcount+=1
			gamestate=4
			return
		end

		moveplayer()
		updateanimations()

	elseif gamestate==4 then

		stateincrementor+=12

		if stateincrementor > statetransitionmax then
			stateincrementor=0
			gamestate=1

			if isrestart==false then
				level+=1
			end

			_init()
		end
	end
end

function drawhud()
	rectfill(0,120,127,127,0)
	rect(0,120,127,128,7)
	
	print("level "..level,2,122,7)
	print("remaining "..blockcount,76,122,7)
end

function drawactors()
	
	spr(portalsprites[portalcounter],portal.x,portal.y)
	
	for i=1,blockcount,1 do
		spr(005,blocks[i].x,blocks[i].y)
	end
	
	spr(playerspr,player.x,player.y)
end

function _draw()
	if gamestate==1 then
		rectfill(0,0,127,127,0)
		print("level "..level,40,58,7)
		print("restarts "..restartcount,40,66,7)
	end

	if gamestate==3 or gamestate==2 or gamestate==4 then
		rectfill(0,0,127,119,3)
		map(colstart, rowstart, 0, 0, 16, 15)
		drawactors()
		drawhud()
	end

	if gamestate==4 then
		rectfill(0,0,stateincrementor,127,0)
	elseif gamestate==2 then
		rectfill(0,0,statetransitionmax-stateincrementor,127,0)
	end
end

